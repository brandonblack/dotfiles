[core]
  whitespace = fix,space-before-tab,tab-in-indent,trailing-space

[user]
  email = brandonmblack@gmail.com
  name  = Brandon Black

[github]
  user = brandonblack

[push]
  default = tracking

[branch]
  autosetuprebase = always

[color]
  ui          = auto
  interactive = auto

[hub]
  http-clone = false
  protocol   = https

[url "git@github.com:"]
  insteadOf = "gh:"
  pushInsteadOf = "github:"
  pushInsteadOf = "git://github.com/"

[url "git://github.com/"]
  insteadOf = "github:"

[url "git@gist.github.com:"]
  insteadOf = "gst:"
  pushInsteadOf = "gist:"
  pushInsteadOf = "git://gist.github.com/"

[url "git://gist.github.com/"]
  insteadOf = "gist:"

[url "git@heroku.com:"]
  insteadOf = "heroku:"